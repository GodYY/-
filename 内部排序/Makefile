EXE = straight_insertion_sort binary_insertion_sort bubble_sort quick_sort

straight_insertion_sort:
	gcc straight_insertion_sort.c \
	-o straight_insertion_sort

binary_insertion_sort:
	gcc binary_insertion_sort.c \
	-o binary_insertion_sort

bubble_sort:
	gcc bubble_sort.c \
	-o bubble_sort

quick_sort:
	gcc quick_sort.c \
	-o quick_sort

clean:
	rm -rf ${EXE}

all: ${EXE}